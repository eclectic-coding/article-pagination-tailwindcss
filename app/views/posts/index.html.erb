<p id="notice"><%= notice %></p>

<h1 class="text-2xl font-semibold my-2">All Posts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <h2 class="text-xl font-semibold"><%= post.title %></h2>
      <p class="m-1 pb-4"><%= post.content %></p>
    <% end %>
  </tbody>
</table>

<br>
